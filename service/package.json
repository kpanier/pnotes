{
  "name": "notesapi",
  "version": "1.0.0",
  "description": "Notes API",
  "main": "main.js",
  "scripts": {
    "test": "nyc mocha -u tdd -r ts-node/register src/**/*.test.ts",
    "tsc": "tsc --watch",
    "build": "tsc",
    "serve": "nodemon dist/main.js"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.test.ts",
      "**/*.js"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": true
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^2.2.48",
    "@types/node": "^8.0.53",
    "mocha": "^5.0.0",
    "mongo-in-memory": "0.0.5",
    "nyc": "^11.4.1",
    "ts-node": "^4.1.0",
    "typemoq": "^2.1.0",
    "typescript": "~2.7.1"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "diff-match-patch": "^1.0.0",
    "express": "^4.16.2",
    "hal": "^1.2.0",
    "jsonwebtoken": "^8.1.0",
    "mongodb": "^2.2.33",
    "multer": "^1.3.0",
    "nodemon": "^1.11.0",
    "ts-md5": "^1.2.2"
  }
}

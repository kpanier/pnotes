{
  "name": "personal-notes",
  "displayName": "personal-notes",
  "description": "",
  "version": "0.0.3",
  "publisher": "aysada",
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:pnote.openNote",
    "onCommand:pnote.addNote",
    "onView:pnote"
  ],
  "main": "./out/extension",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "pnote",
          "name": "Personal Notes"
        }
      ]
    },
    "commands": [
      {
        "command": "pnote.openNote",
        "title": "Open Personal note"
      },
      {
        "command": "pnote.addNote",
        "title": "Add Personal note",
        "icon": {
          "light": "resources/light/document.svg",
          "dark": "resources/dark/document.svg"
        }
      },
      {
        "command": "pnote.deleteNote",
        "title": "Delete Personal note"
      },
      {
        "command": "pnote.showHistory",
        "title": "Show history"
      },
      {
        "command": "pnote.syncNote",
        "title": "Sync Perosnal notes",
        "icon": {
          "light": "resources/light/refresh.svg",
          "dark": "resources/dark/refresh.svg"
        }
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "pnote.addNote",
          "when": "view == pnote",
          "group": "navigation"
        },
        {
          "command": "pnote.syncNote",
          "when": "view == pnote",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "pnote.deleteNote",
          "when": "view == pnote"
        },
        {
          "command": "pnote.showHistory",
          "when": "view == pnote"
        }
      ]
    },
    "configuration": [
      {
        "title": "Personal note username",
        "properties": {
          "pnote.username": {
            "type": "string"
          }
        }
      },
      {
        "title": "Personal note base url",
        "properties": {
          "pnote.baseurl": {
            "type": "string",
            "default": "http://localhost:3000"
          }
        }
      },
      {
        "title": "Personal note proxy url",
        "properties": {
          "pnote.proxyurl": {
            "type": "string",
            "default": "http://proxy"
          }
        }
      },
      {
        "title": "Personal note proxy username",
        "properties": {
          "pnote.proxyusername": {
            "type": "string",
            "default": "user"
          }
        }
      },
      {
        "title": "Personal note proxy password",
        "properties": {
          "pnote.proxypassword": {
            "type": "string",
            "default": "pwd"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.6.1",
    "vscode": "^1.1.6",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "diff-match-patch": "^1.0.0",
    "ts-md5": "^1.2.2",
    "typed-rest-client": "^0.15.1"
  }
}
